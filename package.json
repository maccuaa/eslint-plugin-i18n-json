{
  "name": "@maccuaa/eslint-plugin-i18n-json",
  "version": "4.0.0",
  "description": "Fully extendable eslint plugin for JSON i18n translation files.",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "run-s clean rome test compile",
    "clean": "rimraf dist coverage",
    "compile": "tsc",
    "format": "rome format src",
    "lint": "rome check src",
    "rome": "rome ci src",
    "test": "vitest run --coverage"
  },
  "author": "Andrew MacCuaig <andrewmaccuaig@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:maccuaa/eslint-plugin-i18n-json"
  },
  "bugs": {
    "url": "https://github.com/maccuaa/eslint-plugin-i18n-json/issues"
  },
  "keywords": [
    "eslint",
    "json",
    "i18n",
    "translations",
    "internationalization",
    "intl",
    "linter"
  ],
  "peerDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.54.1",
    "ansi-colors": "^4.1.3",
    "jest-diff": "^28.1.3",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.set": "^4.3.2",
    "parse-json": "^5.2.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.0",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^18.15.0",
    "@types/parse-json": "^4.0.0",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-istanbul": "^0.29.8",
    "npm-run-all2": "^6.0.5",
    "rimraf": "^5.0.0",
    "rome": "^12.0.0",
    "typescript": "^4.8.4",
    "vitest": "^0.29.2"
  }
}
